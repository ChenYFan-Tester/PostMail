{
  "name": "POST邮件者",
  "description": "利用Heroku实现POST发送邮件",
  "image": "heroku/python",
  "repository": "https://github.com/ChenYFan-Tester/PostMail",
  "env": {
    "SMTP_username": {
      "description": "邮件SMTP登录用户名",
